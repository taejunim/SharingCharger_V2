<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.drawerlayout.widget.DrawerLayout
        android:id="@+id/drawer_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.coordinatorlayout.widget.CoordinatorLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#F2F2F2">

            <RelativeLayout
                android:id="@+id/layout_map"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">

                <net.daum.mf.map.api.MapView
                    android:id="@+id/map_view"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent" />

                <RelativeLayout
                    android:id="@+id/layout_top1"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="10dp"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:padding="1dp">

                    <RelativeLayout
                        android:id="@+id/btn_menu"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:background="@drawable/shadow_background"
                        android:padding="7dp">

                        <ImageView

                            android:layout_width="32dp"
                            android:layout_height="32dp"
                            android:layout_centerVertical="true"
                            android:background="@mipmap/menu"
                            android:button="@android:color/transparent"
                            android:contentDescription="@string/image_description" />

                    </RelativeLayout>

                    <EditText
                        android:id="@+id/edit_search"
                        style="@style/edit_txt2"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:layout_marginStart="20dp"
                        android:layout_toEndOf="@+id/btn_menu"
                        android:textSize="12dp"
                        android:importantForAutofill="no"
                        android:inputType="text" />

                </RelativeLayout>

                <!-- location_img-->
                <FrameLayout
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_below="@+id/layout_top1"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="15dp"
                    android:background="@drawable/border_white_30">

                    <ImageView
                        android:id="@+id/btn_current_location"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_below="@+id/layout_top1"
                        android:layout_alignParentEnd="true"
                        android:layout_gravity="center"
                        android:button="@android:color/transparent"
                        android:src="@mipmap/current_location_off" />

                </FrameLayout>
                <!-- location_img-->

                <!-- 예약없을때-->
                <RelativeLayout
                    android:id="@+id/layout_bottom_tab"
                    android:layout_width="match_parent"
                    android:layout_height="300dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="15dp"
                    android:layout_marginEnd="15dp"
                    android:background="@drawable/edit_text_border"
                    android:visibility="visible">

                    <FrameLayout
                        android:id="@+id/charger_image_layout"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="15dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        android:background="@drawable/border_blue_50">

                        <ImageView
                            android:id="@+id/image_clock"
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:layout_centerVertical="true"
                            android:layout_marginTop="5dp"
                            android:background="@mipmap/battery_img"
                            android:contentDescription="@string/image_description" />

                    </FrameLayout>
                    <RelativeLayout
                        android:id="@+id/layout_charging_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="10dp">

                        <TextView
                            android:id="@+id/txt_charging_time_start"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/search_condition_title_total"
                            android:textColor="@color/black"
                            android:textSize="22dp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/txt_charging_time"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/txt_charging_time_start"
                            android:text="@string/minute30"
                            android:textColor="@color/black"
                            android:textSize="22dp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/txt_charging_time_end"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/txt_charging_time"
                            android:text="@string/search_condition_title_charging"
                            android:textColor="@color/black"
                            android:textSize="22dp"
                            android:textStyle="bold" />
                    </RelativeLayout>

                    <TextView
                        android:id="@+id/txt_charging_term"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/layout_charging_time"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="5dp"
                        android:textColor="@color/black"
                        android:textSize="16sp"
                        tools:text="7/24 (금) 22:30 ~ 23:00" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@+id/charger_image_layout"
                        android:background="@color/bright_white"/>

                    <RelativeLayout
                        android:id="@+id/main_charger_list"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@+id/charger_image_layout"
                        android:layout_margin="10dp"
                        android:background="@color/white">

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/main_charger_recycler_View"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"/>

                    </RelativeLayout>

                </RelativeLayout>
                <!-- 예약없을때-->

                <!-- 예약있을때-->
                <RelativeLayout
                    android:id="@+id/layout_reservation_info"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="15dp"
                    android:layout_marginEnd="15dp"
                    android:background="@drawable/button_border_blue"
                    android:visibility="invisible">

                    <FrameLayout
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginStart="15dp"
                        android:layout_marginEnd="10dp"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/border_thick_blue_50">

                        <ImageView
                            android:id="@+id/image_battery"
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:layout_centerVertical="true"
                            android:layout_marginTop="5dp"
                            android:background="@mipmap/battery_img"
                            android:contentDescription="@string/image_description" />

                    </FrameLayout>

                    <RelativeLayout
                        android:id="@+id/layout_reservation_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="18dp">

                        <TextView
                            android:id="@+id/txt_reservation_time_start"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/search_condition_title_total"
                            android:textColor="@color/white"
                            android:textSize="20dp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/txt_reservation_time"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/txt_reservation_time_start"
                            android:text="10시간 30분"
                            android:textColor="@color/white"
                            android:textSize="20dp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/txt_reservation_time_end"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/txt_reservation_time"
                            android:text="@string/search_condition_title_charging"
                            android:textColor="@color/white"
                            android:textSize="20dp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/layout_reservation_info_recharge_txt"
                            android:layout_width="55dp"
                            android:layout_height="25dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="2dp"
                            android:layout_toEndOf="@+id/txt_reservation_time_end"
                            android:background="@drawable/border_mint_30"
                            android:gravity="center"
                            android:textSize="14dp"
                            android:text="예약"
                            android:textColor="@color/white"
                            android:textStyle="bold" />

                    </RelativeLayout>

                    <TextView
                        android:id="@+id/txt_reservation_term"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/layout_reservation_time"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="8dp"
                        android:text="7/24 (금) 22:30 ~ 23:00"
                        android:textColor="@color/white"
                        android:textSize="17dp" />

                    <ImageView
                        android:id="@+id/img_dropdown_main"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="10dp"
                        android:button="@android:color/transparent"
                        android:contentDescription="@string/image_description"
                        android:padding="15dp"
                        android:src="@mipmap/btn_dropdown_white" />

                </RelativeLayout>
                <!-- 예약있을때-->

                <!--  예약 클릭시-->
                <RelativeLayout
                    android:id="@+id/layout_reservation_detail_info"
                    android:layout_width="match_parent"
                    android:layout_height="350dp"
                    android:layout_alignParentBottom="true"
                    android:background="@drawable/edit_text_border"
                    android:visibility="invisible">

                    <ImageView
                        android:id="@+id/reservation_detail_close_img"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        android:layout_margin="15dp"
                        android:background="@mipmap/icon_x"
                        android:button="@android:color/transparent" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@+id/reservation_detail_close_img">

                        <TextView
                            android:id="@+id/reservation_detail_total_time_1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:layout_marginEnd="5dp"
                            android:text="총"
                            android:textColor="@color/black"
                            android:textSize="21dp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/reservation_detail_total_time"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toEndOf="@+id/reservation_detail_total_time_1"
                            android:text="30분"
                            android:textColor="@color/black"
                            android:textSize="21dp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/reservation_detail_total_time_2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:layout_toEndOf="@+id/reservation_detail_total_time"
                            android:text="충전"
                            android:textColor="@color/black"
                            android:textSize="21dp"
                            android:textStyle="bold" />

                        <Button
                            android:id="@+id/main_recharging_txt"
                            android:layout_width="80dp"
                            android:layout_height="25dp"
                            android:background="@drawable/border_mint_30"
                            android:text="충전중"
                            android:textStyle="bold"
                            android:layout_alignParentEnd="true"
                            android:layout_marginStart="0dp"
                            android:layout_marginEnd="20dp"
                            android:visibility="invisible"
                            android:textColor="@color/white"/>

                        <TextView
                            android:id="@+id/reservation_detail_detail_time"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/reservation_detail_total_time_1"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginEnd="20dp"
                            android:text="7/20 (금) 22:30 ~ 23:00"
                            android:textColor="@color/black" />

                        <View
                            android:id="@+id/reservation_detail_view"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_below="@+id/reservation_detail_detail_time"
                            android:layout_marginTop="30dp"
                            android:layout_marginBottom="40dp"
                            android:background="@color/dark_gray" />

                        <TextView
                            android:id="@+id/txt_chgr__detail_nm"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/reservation_detail_view"
                            android:layout_marginStart="20dp"
                            android:layout_marginEnd="0dp"
                            android:text="관광대학교 주차장"
                            android:textColor="@color/black"
                            android:textSize="21dp"
                            android:textStyle="bold" />

                        <ImageButton
                            android:id="@+id/image_favorite_detail"
                            android:layout_width="32dp"
                            android:layout_height="32dp"
                            android:layout_below="@+id/reservation_detail_view"
                            android:layout_marginStart="10dp"
                            android:layout_toEndOf="@+id/txt_chgr__detail_nm"
                            android:background="@mipmap/star_off"
                            android:contentDescription="@string/image_description" />

                        <TextView
                            android:id="@+id/txt_chgr_addr_detail_nm"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/txt_chgr__detail_nm"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginEnd="0dp"
                            android:text="제주 제주시 관광대학로 111(아라일동)"
                            android:textColor="@color/black"
                            android:textSize="15dp" />

                        <TextView
                            android:id="@+id/txt_chgr_detail_addr_detail_nm"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/txt_chgr_addr_detail_nm"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="5dp"
                            android:layout_marginEnd="0dp"
                            android:text="제주 제주시 관광대학로 111(아라일동)"
                            android:textColor="@color/gray"
                            android:textSize="15sp"/>


                        <FrameLayout
                            android:id="@+id/btn_navi_detail"
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:layout_alignBaseline="@id/txt_chgr_addr_detail_nm"
                            android:layout_alignParentEnd="true"
                            android:layout_marginStart="0dp"
                            android:layout_marginEnd="20dp"
                            android:background="@drawable/border_blue_50"
                            android:contentDescription="@string/image_description">

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:src="@mipmap/roadmap"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="6dp"
                                android:layout_marginEnd="0dp"/>

                        </FrameLayout>

                        <TextView
                            android:id="@+id/txt_chgr_detail_amt_key"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/txt_chgr_detail_addr_detail_nm"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginEnd="0dp"
                            android:text="@string/main_charge_amt_key"
                            android:textColor="@color/black"
                            android:textSize="15dp" />

                        <TextView
                            android:id="@+id/txt_chgr_detail_amt_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/txt_chgr_detail_addr_detail_nm"
                            android:layout_marginTop="10dp"
                            android:layout_toEndOf="@+id/txt_chgr_detail_amt_key"
                            android:textColor="@color/black"
                            android:textSize="15dp"
                            tools:text="178원" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true">

                            <Button
                                android:id="@+id/chgr_detail_cancel"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@drawable/button_border_red"
                                android:text="예약 취소"
                                android:textColor="@color/white"
                                android:textSize="17dp"
                                android:textStyle="bold" />

                            <Button
                                android:id="@+id/chgr_detail_start"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@drawable/button_border_blue"
                                android:text="충전 시작"
                                android:textColor="@color/white"
                                android:textSize="17dp"
                                android:textStyle="bold" />

                        </LinearLayout>
                    </RelativeLayout>
                </RelativeLayout>
                <!-- 예약 클릭시-->

            </RelativeLayout>

            <!-- 마커 클릭시-->

            <RelativeLayout
                android:id="@+id/layout_chgr_info"
                android:layout_width="match_parent"
                android:layout_height="330dp"
                android:background="@drawable/edit_text_border"
                android:visibility="invisible"
                app:behavior_hideable="false"
                app:behavior_peekHeight="330dp"
                app:layout_behavior="com.google.android.material.bottomsheet.BottomSheetBehavior">

                <View
                    android:id="@+id/view_scroll"
                    android:layout_width="40dp"
                    android:layout_height="10dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/button_border_white_gray" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/view_scroll"
                    android:padding="20dp">

                    <TextView
                        android:id="@+id/txt_chgr_nm"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="관광대학교 주차장"
                        android:textColor="@color/black"
                        android:textSize="21dp"
                        android:textStyle="bold" />

                    <ImageButton
                        android:id="@+id/image_favorite"
                        android:layout_width="32dp"
                        android:layout_height="32dp"
                        android:layout_marginStart="10dp"
                        android:layout_toEndOf="@+id/txt_chgr_nm"
                        android:background="@mipmap/star_off"
                        android:contentDescription="@string/image_description" />

                    <FrameLayout
                        android:id="@+id/btn_navi"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_alignParentEnd="true"
                        android:layout_marginTop="35dp"
                        android:background="@drawable/border_blue_50"
                        android:contentDescription="@string/image_description">

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="50dp"
                            android:src="@mipmap/roadmap"
                            android:layout_marginTop="5dp"
                            android:layout_marginStart="6dp"
                            android:layout_marginEnd="0dp"/>

                    </FrameLayout>

                    <TextView
                        android:id="@+id/txt_chgr_addr_nm"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/txt_chgr_nm"
                        android:layout_marginTop="5dp"
                        android:text="제주 제주시 관광대학로 111(아라일동)"
                        android:textColor="@color/black"
                        android:textSize="15dp" />

                    <TextView
                        android:id="@+id/charger_detail_address_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/txt_chgr_addr_nm"
                        android:layout_marginTop="5dp"
                        android:textColor="@color/gray"
                        android:textSize="15dp" />

                    <TextView
                        android:id="@+id/txt_chgr_amt_key"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/charger_detail_address_text"
                        android:layout_marginTop="10dp"
                        android:text="@string/main_charge_amt_key"
                        android:textColor="@color/black"
                        android:textSize="15dp" />

                    <TextView
                        android:id="@+id/txt_chgr_amt_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/charger_detail_address_text"
                        android:layout_marginTop="10dp"
                        android:layout_toEndOf="@+id/txt_chgr_amt_key"
                        android:textColor="@color/black"
                        android:textSize="15dp"
                        tools:text="178원" />

                    <TextView
                        android:id="@+id/txt_all_time"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/txt_chgr_amt_value"
                        android:layout_marginTop="40dp"
                        android:gravity="center"
                        android:text="항시 충전 가능합니다."
                        android:textColor="@color/black" />

                    <RelativeLayout
                        android:id="@+id/time_available_layout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@id/txt_chgr_amt_value">

                    <TextView
                        android:id="@+id/txt_time_available_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:text="이용 가능 시간"
                        android:textColor="@color/black"
                        android:textSize="18dp"/>


                    <!-- 예약시간텍스트표시-->
                    <HorizontalScrollView
                        android:id="@+id/horizon_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/txt_time_available_title">

                    <LinearLayout
                        android:id="@+id/layout_chgr_time_available_txt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="25dp"
                        android:gravity="center">

                        <TextView
                            android:layout_width="100dp"
                            android:layout_height="25dp"
                            android:background="@drawable/border_mint_30"
                            android:gravity="center"
                            android:text="17:00~18:30"
                            android:textColor="@color/white"
                            android:textSize="12dp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="100dp"
                            android:layout_height="25dp"
                            android:background="@drawable/border_mint_30"
                            android:gravity="center"
                            android:text="23:00~23:30"
                            android:textColor="@color/white"
                            android:textSize="12dp"
                            android:textStyle="bold" />

                    </LinearLayout>
                    </HorizontalScrollView>
                    <!-- 예약시간텍스트표시-->

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/horizon_layout"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="위 시간대에 이용이 가능합니다."
                        android:textColor="@color/black" />
                    </RelativeLayout>

                </RelativeLayout>


                <Button
                    android:id="@+id/btn_reservation"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="visible"
                    android:layout_alignParentBottom="true"
                    android:background="@drawable/button_border_blue"
                    android:text="@string/go_charge"
                    android:textColor="@color/white"
                    android:textSize="17dp"
                    android:textStyle="bold" />
            </RelativeLayout>

            <!-- 마커 클릭시-->

            <ProgressBar
                android:visibility="invisible"
                android:id="@+id/loading"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"/>

        </androidx.coordinatorlayout.widget.CoordinatorLayout>

        <com.google.android.material.navigation.NavigationView
            android:id="@+id/nav_view"
            android:layout_width="340dp"
            android:layout_height="match_parent"
            android:layout_gravity="start"
            app:headerLayout="@layout/nav_header_main">

            <ExpandableListView
                android:id="@+id/list_slidermenu"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="end"
                android:layout_marginTop="232dp"
                android:background="@color/white"
                android:choiceMode="singleChoice"
                android:divider="#e6e6e6"
                android:dividerHeight="1dp"
                android:groupIndicator="@null"
                app:headerLayout="@layout/nav_header_main" />
        </com.google.android.material.navigation.NavigationView>
    </androidx.drawerlayout.widget.DrawerLayout>
</layout>